{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "react",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "@config/*": ["src/config/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "@assets/*": ["src/assets/*"],
      "@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"],
      "@data/*": ["src/data/*"],
      "@lib/*": ["src/lib/*"],
      "@services/*": ["src/services/*"],
      "@/*": ["src/*"],
      "!/*": ["./*"]
    },
    "types": [
      "astro/client",
      "vite-plugin-pwa/vanillajs",
      "vite-plugin-pwa/info",
      "@cloudflare/workers-types"
    ]
  }
}
